#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double x, a, c, y;

    cout << "Enter x: ";
    cin >> x;
    cout << "Enter a: ";
    cin >> a;
    cout << "Enter c: ";
    cin >> c;

    if (x <= 0) {
        y = 2 * pow(x, 2) + a;
    } else {
        y = (x + 3 * a) * c;
    }

    cout << "Result: Y = " <<y;

}



#include <iostream>
using namespace std;

int main() {
    double x, y;
    cout << "Enter coordinates of the point (x, y): ";
    cin >> x >> y;

    if (x >= 0 && y >= 0 && y <= -x + 2) {
        cout << "The point (" << x << ", " << y << ") is True.";
    } else {
        cout << "The point (" << x << ", " << y << ") is False.";
    }
}
